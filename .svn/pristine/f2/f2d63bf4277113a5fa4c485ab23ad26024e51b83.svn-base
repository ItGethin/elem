    <!-- Demo page code -->

    <style type="text/css">
        #line-chart {
            height:300px;
            width:800px;
            margin: 0px auto;
            margin-top: 1em;
        }
        .brand { font-family: georgia, serif; }
        .brand .first {
            color: #ccc;
            font-style: italic;
        }
        .brand .second {
            color: #fff;
            font-weight: bold;
        }
       .news{
           width:100%;
           padding-left:300px;
           padding-top:50px;
       }
        .news table th{
            width:100px;
            height:30px;
        }
        .news table td{
            width:100px;
            height:30px;
            text-align: center;
        }

        .news table td a{
            display: inline-block;
            margin-right:15px;
            background: right;
        }
        .news table td span{
            display: inline-block;
            margin-right:8px;
        }
        .add{
            display: block;
            height:50px;
            width:50px;
            background:green;
            text-align: center;
            line-height: 50px;
            margin-left:570px;
            margin-top:20px;
        }
        .change{
            width:300px;
            height:300px;
            margin:0 auto;
            margin-top:50px;
        }
        .change input{
            width:300px;
            height:20px;
            outline: none;
        }
        .change .sure{
            width:50px;
            height:30px;
            margin-left:264px;
        }
    </style>

